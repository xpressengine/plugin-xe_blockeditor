{
  "name": "xpressengine-plugin/xe_blockeditor",
  "description": "WP의 Gutenberg를 사용한 블록에디터를 활용하여 멋진 글을 완성하세요!",
  "keywords": ["xpressengine bundle", "XE XeBlockeditor"],
  "version": "1.0.0",
  "support": {
    "email": "developers@xpressengine.com"
  },
  "authors": [
    {
      "name": "XE Developers",
      "email": "developers@xpressengine.com",
      "homepage": "https://store.xehub.io/plugins/xe_blockeditor",
      "role": "Developer"
    }
  ],
  "license": "LGPL-3.0-or-later",
  "type": "xpressengine-plugin",
  "extra": {
    "xpressengine": {
      "title": "XeBlockeditor",
      "icon": "icon.png",
      "component": {
        "editor/xe_blockeditor@xe_blockeditor": {
          "class": "Xpressengine\\Plugins\\XeBlockeditor\\Components\\Editors\\XeBlockeditor",
          "name": "XeBlockeditor",
          "description": "Block editor"
        }
      }
    }
  },
  "require": {
    "xpressengine/xpressengine": "~3.0.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "mockery/mockery": "dev-master@dev"
  },
  "autoload": {
    "psr-4": {
      "Xpressengine\\Plugins\\XeBlockeditor\\": "src/",
      "Xpressengine\\Plugins\\XeBlockeditor\\Components\\": "components/"
    }
  }
}
